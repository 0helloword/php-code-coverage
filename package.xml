  <required>
   <php>
    <min>5.2.7</min>
   </php>
   <pearinstaller>
    <min>1.8.1</min>
   </pearinstaller>
   <package>
    <name>File_Iterator</name>
    <channel>pear.phpunit.de</channel>
    <min>1.1.0</min>
   </package>
   <package>
    <name>Text_Template</name>
    <channel>pear.phpunit.de</channel>
    <min>1.0.0</min>
   </package>
  </required>
  <optional>
   <extension>
    <name>xdebug</name>
    <min>2.0.5</min>
   </extension>
  </optional>
